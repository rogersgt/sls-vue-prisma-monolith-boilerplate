{
  "name": "road-dog-app",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "build": "serverless package",
    "lint": "eslint src",
    "test": "npm run lint && npm run build",
    "start": "nodemon --inspect node_modules/serverless/bin/serverless.js offline start -t",
    "deploy:app:prod": "ENVIRONMENT=prod serverless deploy --force",
    "deploy:migrations:prod": "ENVIRONMENT=prod serverless runRemoteMigrations",
    "deploy": "npm run deploy:migrations:prod && npm run deploy:app:prod",
    "run:remote:migrations:prod": "ENVIRONMENT=prod serverless runRemoteMigrations",
    "db:gen:client": "prisma generate client",
    "db:migrate:dev": "prisma migrate dev && npm run db:gen:client",
    "db:migrate:deploy": "prisma migrate deploy",
    "postinstall": "(cd deploy/plugins/serverless-run-remote-migrations && npm i) && (cd road-dog-frontend && npm i)"
  },
  "author": "Grady T Rogers",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.5",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "chai": "4.3.10",
    "eslint": "8.55.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "mocha": "10.2.0",
    "nodemon": "3.0.2",
    "prisma": "5.6.0",
    "raw-loader": "4.0.2",
    "serverless": "3.38.0",
    "serverless-offline": "13.3.0",
    "serverless-webpack": "5.13.0",
    "ts-loader": "9.5.1",
    "webpack": "5.89.0",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "3.485.0",
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@fortawesome/vue-fontawesome": "3.0.5",
    "@prisma/client": "^5.6.0",
    "axios": "1.6.5",
    "casbin": "5.28.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "date-fns": "3.2.0",
    "express": "4.18.2",
    "google-auth-library": "9.4.2",
    "helmet": "7.1.0",
    "jsonwebtoken": "9.0.2",
    "jwk-to-pem": "2.0.5",
    "pinia-plugin-persistedstate": "3.2.1",
    "serverless-frontend-plugin": "2.0.2",
    "serverless-http": "3.2.0",
    "typescript": "5.3.2",
    "vue": "3.3.9",
    "winston": "3.11.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
